{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-plusplus": 0,
    "max-len": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/webpack.*.js"]}]
  }
}
